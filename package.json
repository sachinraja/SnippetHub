    "@types/tailwindcss": "^2.0.5",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "autoprefixer": "^10.2.6",
    "babel-plugin-superjson-next": "^0.3.0",
    "dotenv-cli": "^4.0.0",
    "eslint": "^7.28.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-next": "^10.2.3",
