{
  "name": "snippethub",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start:prod": "docker-compose -f docker/docker-compose.yml up",
    "test:docker": "NODE_ENV=test docker-compose -f docker/docker-compose.test.yml up -d && dotenv -e src/prisma/.env.test.local prisma migrate deploy && npm run prisma:seed && npm test",
    "test": "jest",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "type:check": "tsc",
    "type:gen": "npm run prisma:gen && npm run graphql:gen",
    "prisma:gen": "prisma generate",
    "prisma:seed": "prisma db seed --preview-feature",
    "nexus:gen": "ts-node -r tsconfig-paths/register --transpile-only src/graphql/schema.ts",
    "graphql-let:gen": "graphql-let",
    "graphql:gen": "npm run nexus:gen && npm run graphql-let:gen",
    "prepare": "husky install"
  },
  "prisma": {
    "schema": "src/prisma/schema.prisma"
  },
  "dependencies": {
